#include <iostream>
#include <string>
using namespace std;

string sorting(string s) {
    int n = s.length();
    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < n - i - 1; j++) {
            if (s[j] > s[j + 1]) {
                char temp = s[j];
                s[j] = s[j + 1];
                s[j + 1] = temp;
       }
     }
    }
    return s;
}

int main() {
    string w1, w2;
    cin >> w1 >> w2;

    if (w1.length() != w2.length()) {
        cout << "NO";
        return 0;
    }

    w1 = sorting(w1);
    w2 = sorting(w2);

    if (w1 == w2)
        cout << "YES";
    else
        cout << "NO";

    return 0;
}
