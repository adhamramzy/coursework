#include <iostream>
using namespace std;

int func(int left, int right) {
    if (left == right) {
        return 0;
   }
    if (left > right) {
        return 1;
   }
    else {
        return -1;
  }
}

int main() {
    int A = 10;
    int B = 5;
    int C = 10;

    int res1 = func(A, B);

    if (res1 == 0) {
        int res2 = func(C, A);

        if (res2 > 0)
            cout << "Fake coin is heavier." << endl;
        else
            cout << "Fake coin is lighter." << endl;
    }
    else {
        int res2 = func(A, C);

        if (res2 == 0) {
            if (res1 > 0)
                cout << "Fake coin is heavier." << endl;
            else
                cout << "Fake coin is lighter." << endl;
        }
        else {
            if (res2 > 0)
                cout << "Fake coin is heavier." << endl;
            else
                cout << "Fake coin is lighter." << endl;
        }
    }

    return 0;
}
